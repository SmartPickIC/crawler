{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "debugpy",  // 예: Python 디버깅 설정
            "request": "launch",
            "program": "${file}",  // 현재 열려 있는 파일 실행
            "console": "integratedTerminal",
            "justMyCode": false,  // 모든 코드 디버깅 가능
            "stopOnEntry": false,
            "env": {
                "PYTHONUNBUFFERED": "1"
            }
        }
    ]
}
